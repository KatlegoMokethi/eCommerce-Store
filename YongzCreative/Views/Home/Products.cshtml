@model IEnumerable<Product>
@{
    ViewData["Title"] = "All Products";
}

<div class="container">
    <h1 id="Hoodies">Products</h1>
    <div class="card-columns">
        @foreach (Product p in Model)
        {
            <partial name="_ProductPartialView" model="p" />
        }
    </div>
</div>